{
  "manifest_version": 3,
  "name": "TidyQA Simple Bug Tracker",
  "version": "1.0",
  "description": "A simple Chrome extension to track bugs based on website URL.",
  "action": {
    "default_popup": "popup.html"
  },
"permissions": ["tabs"],
 "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
